{
  "name": "@jasonkarns/setup-homebrew",
  "version": "0.2.1",
  "private": true,
  "description": "Sets up Homebrew for use in subsequent steps",
  "homepage": "https://github.com/actions/javascript-action#readme",
  "license": "MIT",
  "author": "Jason Karns <jason.karns@gmail.com> (http://jasonkarns.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonkarns/setup-homebrew.git"
  },
  "bugs": {
    "url": "https://github.com/jasonkarns/setup-homebrew/issues"
  },
  "main": "src",
  "scripts": {
    "clean": "git checkout -- dist",
    "lint": "standard --fix",
    "test": "teenytest",
    "posttest": "npm run lint",
    "prepare": "ncc build",
    "preversion": "npm run prepare && test master = $(git symbolic-ref --short HEAD)",
    "version": "git add -- dist",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@actions/core": "^1.1.1",
    "@actions/exec": "^1.0.3",
    "@actions/tool-cache": "^1.3.3"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.20.5",
    "prettier-standard": "^16.2.1",
    "standard": "^14.3.3",
    "teenytest": "^5.3.0"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Homebrew"
  ],
  "standard": {
    "ignore": "dist"
  }
}
